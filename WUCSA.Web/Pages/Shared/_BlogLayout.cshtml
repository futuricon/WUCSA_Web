                                    @*<li class="nav-item">
                                            <a href="#">Pages</a>
                                            <ul class="sub-menu">
                                                <li><a href="">About Us</a></li>
                                            </ul>
                                        </li>*@
                                    <li class="nav-item"><a asp-page="/About">About</a></li>
                                    <li class="nav-item"><a asp-fragment="memberx">Members</a></li>
                                    <li class="nav-item"><a href="#">Events</a></li>
                                    <li class="nav-item"><a href="#">Ranking</a></li>
                                    <li class="nav-item"><a href="#">Gallery </a></li>
                                    <li class="nav-item"><a asp-page="/Blog/List">Blog</a></li>
                                    <li class="nav-item"><a asp-page="/Contacts">Contacts</a></li>
                                    <vc:culture-switcher />
                                </ul>
                            </div>
                            @*<a class="navbar-brand logo" asp-area="" asp-page="/Index">
                                    <img class="logo1" src="~/images/logo/logo.svg" alt="Logo" />
                                </a>*@
                            <!-- navbar collapse -->
                            <partial name="_LoginPartial" />

                            @*<div class="button">
                                    <a href="javacript:" data-toggle="modal" data-target="#login" class="login">
                                        <i class="lni lni-lock-alt"></i> Login
                                    </a>
                                    <a href="javacript:" data-toggle="modal" data-target="#signup" class="btn">Sign Up</a>
                                </div>*@
                        </nav>
                        <!-- navbar -->
                    </div>
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
        <!-- navbar area -->
    </header>
    <!-- End Header Area -->
    <main role="main">
        @RenderBody()
    </main>

    <!-- Start Footer Area -->
    <footer class="footer">
        <div class="footer-top">
            <div class="container">
                <div class="row align-items-center justify-content-center">
                    <div class="col-lg-6 col-12">
                        <div class="download-text">
