                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="contact-head wow fadeInUp mt-lg-5 mt-4" data-wow-delay=".4s" style="visibility: visible; animation-delay: 0.4s; animation-name: fadeInUp;">
            <div class="row">
                <div class="form-main">
                    <form class="form" method="post">
                        <div class="row">
                            <div class="col-lg-6 col-12">
                                <div class="form-group">
                                    <input type="text" asp-for="@Model.AuthorName" placeholder="Your Name" required="required">
                                    <span class="text-danger" asp-validation-for="@Model.AuthorName"></span>
                                </div>
                            </div>
                            <div class="col-lg-6 col-12">
                                <div class="form-group">
                                    <input type="text" asp-for="@Model.MsgSubject" placeholder="Your Subject" required="required">
                                    <span class="text-danger" asp-validation-for="@Model.MsgSubject"></span>
                                </div>
                            </div>
                            <div class="col-lg-6 col-12">
                                <div class="form-group">
                                    <input type="email" asp-for="@Model.AuthorEmail" placeholder="Your Email" required="required">
                                    <span class="text-danger" asp-validation-for="@Model.AuthorEmail"></span>
                                </div>
                            </div>
                            <div class="col-lg-6 col-12">
                                <div class="form-group">
                                    <input type="tel" asp-for="@Model.AuthorPhoneNum" placeholder="Your Phone" required="required">
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-group message">
                                    <span asp-validation-for="@Model.MsgContent" class="text-danger"></span>
                                    <textarea asp-for="@Model.MsgContent" placeholder="Your Message"></textarea>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="form-group button">
                                    <button type="submit" class="btn ">Submit Message</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>
</section>

